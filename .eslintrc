{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-var": "off",
    "no-console": "off",
    "no-undef": "warn",
    "no-param-reassign": "off",
    "object-curly-newline": "off",
    "implicit-arrow-linebreak": "off",
    "no-nested-ternary": "off",
    "camelcase": "off",
    "no-plusplus": "off"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ]
}